        android:text="D06"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button34"
        android:textStyle="bold"
        android:background="@color/colorPrimaryDark"
        android:layout_alignBaseline="@+id/button33"
        android:layout_alignBottom="@+id/button33"
        android:layout_alignLeft="@+id/button32"
        android:layout_alignStart="@+id/button32" />

    <TextView
        android:text="4-Wheeler Parking Slots"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView31"
        android:textStyle="bold"
        android:textSize="24sp"
        android:layout_marginTop="17dp"
        android:textColor="@color/colorPrimary"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="Proceed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/button35"
        android:layout_toEndOf="@+id/button35"
        android:layout_marginBottom="20dp"
        android:id="@+id/button57"
        android:background="@android:color/holo_orange_light"
        android:textStyle="bold" />
