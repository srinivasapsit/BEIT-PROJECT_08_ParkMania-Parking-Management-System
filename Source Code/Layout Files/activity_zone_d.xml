        android:layout_marginTop="34dp"
        android:id="@+id/button31"
        android:textStyle="bold"
        android:background="@color/colorPrimaryDark" />

    <Button
        android:text="D04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button31"
        android:layout_alignLeft="@+id/button30"
        android:layout_alignStart="@+id/button30"
        android:id="@+id/button32"
        android:textStyle="bold"
        android:background="@color/colorPrimaryDark" />

    <Button
        android:text="D05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button31"
        android:layout_alignLeft="@+id/button31"
        android:layout_alignStart="@+id/button31"
        android:layout_marginTop="41dp"
        android:id="@+id/button33"
        android:textStyle="bold"
        android:background="@color/colorPrimaryDark" />

    <Button
        android:text="D07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button33"
        android:layout_alignLeft="@+id/button33"
        android:layout_alignStart="@+id/button33"
        android:layout_marginTop="43dp"
        android:id="@+id/button35"
        android:textStyle="bold"
        android:background="@color/colorPrimaryDark" />

